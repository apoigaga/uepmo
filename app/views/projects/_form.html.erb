                 <%= f.text_field :lead_director,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
                  </div>   
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="programme-director">Programme Director</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :programme_manager,:size=>"57",:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
               
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="project-director">Project Director</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :project_manager,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="programme">Programme</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :programme,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="transformation">Transformation</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :Transformation,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="components">Components</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :Components,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
