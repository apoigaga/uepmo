                 <%= f.text_field :KPI_UiTM_Berkaitan,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="deliverables">Deliverables</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_area :deliverables,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-milestone">First Milestone</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :first_milestone,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="impact">Impact</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :impact,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="budget">Budget</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                 <%= f.text_field :budget,:required =>"required",class: :"form-control col-md-7 col-xs-12" %>
              </div>
              <br><br>

              <div class="field">
                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="latihan">Latihan</label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
