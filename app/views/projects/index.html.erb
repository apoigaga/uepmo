<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>
<div class=class="col-md-15 col-sm-3">
                            <div class="x_panel">
                                <div class="x_title">
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Project name</th>
                                                <th>Start date</th>
                                                <th>Finish date</th>
                                                <th>Lead director</th>
                                                <th>Programme manager</th>
                                                <th>Project manager</th>
                                                <th>Action</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                        <% @projects.each do |project| %>
                                            <tr>
                                                <th scope="row">*</th>
                                                <td><%= project.projectname %></td>
                                                <td><%= project.startdate %></td>
                                                <td><%= project.finishdate %></td>
                                                <td></td>
                                                <td><%= project.programmemanager %></td>
                                                <td><%= project.projectmanager %></td>
                                                <td>
                                                    
                                                    <%= link_to 'Show', project, :class =>"btn btn-success btn-xs" %>
                                                    <%= link_to 'Edit', edit_project_path(project), :class =>"btn btn-info btn-xs" %>
                                                    <%= link_to 'Destroy', project,:class =>"btn btn-danger btn-xs", method: :delete, data: { confirm: 'Are you sure?' } %>
                                                </td>
                                            <tr>
                                           <% end %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
 </div>
<br>

<%= link_to 'New Project', new_project_path,:class =>"btn btn-primary" %>
